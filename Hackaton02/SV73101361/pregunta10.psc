Algoritmo pregunta10
    Definir num Como Entero;

    Escribir "Ingrese un numero entero:";
    leer num;

    si num % 2 = 0 Entonces
        Escribir "El numero: ", num, " es par";
    SiNo
        Escribir "El numero: ", num, " es impar";
    FinSi

